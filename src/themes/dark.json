{
  "name": "site-dark",
  "type": "dark",
  "colors": {
    "editor.background": "#24292e",
    "editor.foreground": "#d4d4d8"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#71717a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#eab308"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#4ade80"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#f59e0b"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#a78bfa"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#22d3ee"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#d4d4d8"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#a1a1aa"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#eab308"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#a78bfa"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "punctuation"
      ],
      "settings": {
        "foreground": "#a1a1aa"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#d4d4d8"
      }
    }
  ]
}
