---
interface Props {
  size?: number;
  class?: string;
  [key: string]: any;
}
const { size = 16, class: className, ...attrs } = Astro.props;
---

<svg xmlns="http://www.w3.org/2000/svg" width={size} height={size} viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class={className} {...attrs}>
  <circle cx="12" cy="12" r="10" />
  <polyline points="12 6 12 12 16 14" />
</svg>
